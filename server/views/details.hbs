<div class="place">
  <img class="img-circle" src="{{image}}" alt="beautiful location">
  <h2>{{placeName}}</h2>
  <div>Country: {{country}}</div>
  <div>Currency: {{currency}}</div>
  <div>Language: {{language}}</div>
  <div class="dscrpt">Description: {{description}}</div>
  <a href="/places/{{placeName}}/edit">Edit</a>
</div>
